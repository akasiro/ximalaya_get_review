   "outputs": [],
   "source": [
    "docpath = 'data'\n",
    "filelist = os.listdir(docpath)\n",
    "df = pd.DataFrame()\n",
    "for i in filelist:\n",
    "    tmp = pd.read_csv(os.path.join(docpath,i))\n",
    "    df = df.append(tmp)\n",
    "df.to_csv('data.csv', index = False)"
   ]
